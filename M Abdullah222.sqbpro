    DeptNo    INT PRIMARY KEY,
    DeptName  VARCHAR(100) NOT NULL,
    Location  VARCHAR(100)
);

CREATE TABLE Employee (
    EmpID   INT PRIMARY KEY,
    EmpName VARCHAR(100) NOT NULL,
    Job     VARCHAR(50),
    Salary  DECIMAL(10,2),
    DeptNo  INT,
    FOREIGN KEY (DeptNo) REFERENCES Department(DeptNo)
);
</sql><sql name="SQL 2">SELECT * 
FROM Employee;
</sql><sql name="SQL 3">SELECT EmpName, Salary 
FROM Employee;
</sql><sql name="SQL 4">SELECT E.EmpName, E.Job, D.DeptName
FROM Employee E
JOIN Department D ON E.DeptNo = D.DeptNo;
</sql><sql name="SQL 5">CREATE VIEW HighSalaryEmployees AS
SELECT EmpID, EmpName, Job, Salary, DeptNo
FROM Employee
WHERE Salary &gt; 30000;
</sql><current_tab id="4"/></tab_sql></sqlb_project>
